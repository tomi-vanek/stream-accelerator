{
    "parserOptions": {
        "ecmaVersion": 7
    },
    "rules": {
        "indent": "off",
        "no-console": "off",
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}

